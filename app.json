{
  "expo": {
    "name": "yom.app",
    "slug": "yom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yom.app",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assets": ["./assets/sounds/notification/notificaion_sound_1.wav"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yom.office.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.yom.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "badge": true,
          "sounds": ["./assets/sounds/notification/notificaion_sound_1.wav"]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Aloow yom to access your camera",
          "cameraPermission": "Allow yom to access your camera"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.330776642226-97jaa538gdc5bve38kvikm3ik9gndldr"
        }
      ],
      ["expo-router"],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/WantedSans-Medium.otf",
            "./assets/fonts/WantedSans-Regular.otf",
            "./assets/fonts/WantedSans-Bold.otf",
            "./assets/fonts/WantedSans-SemiBold.otf",
            "./assets/fonts/WantedSans-ExtraBold.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ccf0e8ca-7524-4bd4-892b-ee2d1137a7de"
      }
    }
  }
}
