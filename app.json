{
  "expo": {
    "name": "yom.app",
    "slug": "yom",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yom.app",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assets": ["./assets/sounds/notification/notificaion_sound_1.wav"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yom.office.app",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This allows yom.app to provide you with a better ads experience."
      }
    },
    "android": {
      "package": "com.yom.office.app"
    }
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "permissions": ["android.permission.RECORD_AUDIO"],
    "package": "com.yom.app"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./assets/images/icon.png",
        "color": "#ffffff",
        "defaultChannel": "default",
        "badge": true,
        "sounds": ["./assets/sounds/notification/notificaion_sound_1.wav"]
      }
    ],
    [
      "expo-image-picker",
      {
        "photosPermission": "Aloow yom to access your camera",
        "cameraPermission": "Allow yom to access your camera"
      }
    ],
    [
      "@react-native-google-signin/google-signin",
      {
        "iosUrlScheme": "com.googleusercontent.apps.330776642226-97jaa538gdc5bve38kvikm3ik9gndldr"
      }
    ],
    ["expo-router"],
    [
      "expo-font",
