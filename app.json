{
  "expo": {
    "name": "yom",
    "slug": "yom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yom.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Aloow yom to access your camera",
          "cameraPermission": "Allow yom to access your camera"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.330776642226-97jaa538gdc5bve38kvikm3ik9gndldr"
        }
      ],
      ["expo-router"],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/WantedSans-Medium.otf",
            "./assets/fonts/WantedSans-Regular.otf",
            "./assets/fonts/WantedSans-Bold.otf",
            "./assets/fonts/WantedSans-SemiBold.otf",
            "./assets/fonts/WantedSans-ExtraBold.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
